	color: #ffffff;
}
span.resize {
	height: 31px;
	line-height: 31px;
	display: block;
	float: left;
	width: 28px;
	background: image-url("/assets/fb_app/images/player/ico-resize.png") 4px 9px no-repeat;
	color: #ffffff;
}
span.exresize {
	height: 31px;
	line-height: 31px;
