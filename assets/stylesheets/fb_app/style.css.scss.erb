	font-weight: normal;
	font-style: normal;
}
body {
	font-family: Helvetica, Arial, sans-serif;
	font-size: 11px;
	background: repeat #ffffff;
}
a:hover { position:relative; }
.control-wraper {
	margin: 0;
	padding: 0;
	height: 31px;
	background: #000;
	background-color: #000;
	display: block;
	width: 100%;
	position:relative;
	z-index:99;
}
.clearfix:after {
	content: ".";
	display: block;
	clear: both;
	visibility: hidden;
	line-height: 0;
	height: 0;
}
html[xmlns] .clearfix {
	display: block;
}
* html .clearfix {
	height: 1%;
}
/* Video Player Related */
.wraper-large {
	margin: 0 auto;
	padding: 0;
	width: 100%;
	position:relative;
}
.player-body {
	background-color: #000000;
	width: 100%;
	height: 410px;
	position: relative;
}
ul.play, ul.link {
	margin: 0;
	padding: 0;
	list-style: none;
}
ul.play li, ul.link li {
	margin: 0;
	padding: 0 2px 0 0px;
	height: 31px;
	float: left;
	background: image-url("/assets/fb_app/images/player/sep.png") right 0 no-repeat;
}
ul.link li {
	background: none;
}
ul.play li a, ul.play li a:visited, ul.link li a, ul.link li a:visited {
	padding: 0 10px;
	height: 31px;
	line-height: 31px;
	display: block;
	float: left;
	color: #ffffff;
	text-decoration: none;
	font-weight: bold;
}
ul.link li a {
	padding: 0 15px;
	color: #979797;
}
ul.play li a:hover, ul.link li a:hover, ul.play li a.selected, ul.link li a.selected {
	background: image-url("/assets/fb_app/images/player/hover.png") 0 1px repeat-x;
}
ul.icos li {
	padding: 0 !important;
	background: image-url("/assets/fb_app/images/player/sep.png") right 0 no-repeat;
	position:relative;
}
ul.icos li:last-child{
	/*background:none;*/
}
ul.icos li a {
	padding: 0 5px 0 6px !important;
}
span.btnpreview_profile, span.btnabout_profile {
	color: #FFFFFF;
    float: left;
    margin:6px 5px 0;
	font-weight:bold;
	font-size:11px;
	text-transform:none;
	cursor:pointer;
 }
/*.control-wraper .play */span.btnpreview, /*.control-wraper .play */span.btnabout {
	color: #FFFFFF;
    float: left;
    margin:10px 5px 0;
	font-weight:bold;
	font-size:11px;
	text-transform:none;
	cursor:pointer;
 }
.fl {
	float: left;
}
.fr {
	float: right;
}
span.act {
	height: 31px;
	line-height: 31px;
	display: block;
	float: left;
	width: 14px;
}

span.pplay {
	background: image-url("/assets/fb_app/images/player/ico-play.png") center center no-repeat;
}
span.ico-pause {
	background: image-url("/assets/fb_app/images/player/ico-pause.png") center center no-repeat;
}
span.volume {
	height: 31px;
	line-height: 31px;
