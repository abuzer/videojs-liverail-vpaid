@charset "utf-8";
/* CSS Document */
@font-face {
	font-family: 'helvetica_neueregular';
	src: url('/fb_app/fonts/helveticaneue-webfont.eot');
	src: url('/fb_app/fonts/helveticaneue-webfont.eot?#iefix') format('embedded-opentype'), url('/fb_app/fonts/helveticaneue-webfont.woff') format('woff'), url('/fb_app/fonts/helveticaneue-webfont.ttf') format('truetype'), url('/fb_app/fonts/helveticaneue-webfont.svg#helvetica_neueregular') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'helvetica_neue-medium';
	src: url('/fb_app/fonts/helveticaneue-medium-webfont.eot');
	src: url('/fb_app/fonts/helveticaneue-medium-webfont.eot?#iefix') format('embedded-opentype'), url('/fb_app/fonts/helveticaneue-medium-webfont.woff') format('woff'), url('/fb_app/fonts/helveticaneue-medium-webfont.ttf') format('truetype'), url('/fb_app/fonts/helveticaneue-medium-webfont.svg#helvetica_neueregular') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'helvetica_neue-bold';
	src: url('/fb_app/fonts/helveticaneue-bold-webfont.eot');
	src: url('/fb_app/fonts/helveticaneue-bold-webfont.eot?#iefix') format('embedded-opentype'), url('/fb_app/fonts/helveticaneue-bold-webfont.woff') format('woff'), url('/fb_app/fonts/helveticaneue-bold-webfont.ttf') format('truetype'), url('/fb_app/fonts/helveticaneue-bold-webfont.svg#helvetica_neueregular') format('svg');
	font-weight: normal;
	font-style: normal;
}
body {
	font-family: Helvetica, Arial, sans-serif;
	font-size: 11px;
	background: url("<%=asset_path "/fb_app/images/bg-body.png"%>") 0 0 repeat;
}
body.publisher-bg {
   background: none;
}
.inner-body {
	margin: 0;
	padding: 0;
}
header {
	margin: 0;
	padding: 0;
	height: 54px;
	background:transparent;
	/*background: url(/assets/fb_app/images/header-top.png) 0 0 repeat-x;*/
	position: relative;
	border-bottom:2px solid #133783;
}
section {
	margin: 0 auto;
	padding: 0;/*width: 664px;*/
}
.content-section {
	margin-top: -7px;
}
.content-section section {
	padding: 15px;
	width: 95.7%;/*border: 1px solid #cccccc;
	border-top: 0;
	background: #fff;*/
}
.logo-section {
	margin: 10px 0 0 15px;
	width: 167px;
	float: left;
}
.user-comments-panel {
	margin: 0 0 18px 0;
	padding: 0;
	clear: both;
}
.user-row {
	margin: 0 0 18px 0;
	padding: 0;
	clear: both;
}
.user-row .user-img {
	float: left;
	width: 50px;
	height: 50px;
	margin-right: 10px;
}
.user-data {
	width: 150px;
	float: right;
	text-align: right;
	margin: 10px 15px 0 0;
}
.user-image {
	width: 32px;
	height: 32px;
	float: left;
}
.user-data .user-image img {
	width: 32px;
	height: 32px;
	border: 1px solid #999999;
	vertical-align: middle;
	margin-right: 5px;
}
.user-data a {
	margin-left:10px;
	margin-top:10px;
	width:108px;
	overflow:hidden;
	color: #fff;
	text-decoration: none;
	font-size: 12px;
	font-family: 'helvetica_neueregular', Arial, Helvetica, sans-serif;
	float:left;
	text-align:left;
}
.video-data {
	width: 75%;
	float: left;
}
.video-data h2 {
	margin: 0;
	padding: 5px 0 0 0;
	font-size: 18px;
	font-family: 'helvetica_neue-bold', Arial, Helvetica, sans-serif;
	color: #666666;
}
.video-data h2 a, .video-data h2 a:visited {
	color: #666666;
}
.video-data h2 a:hover {
	color: #333333;
}
.release-data {
	margin-top: 5px;
	font-size: 12px;
	font-family: 'helvetica_neueregular', Arial, Helvetica, sans-serif;
	color: #aaaaaa;
}
.btn-upload {
	margin-top: 10px;
	width: 116px;
	float: right;
}
